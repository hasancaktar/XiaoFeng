{
  "sql.zhuovi.cn": [
    {
      /*数据库驱动*/
      "ProviderType": "SQLite",
      /*数据库命名空间*/
      "DbNameSpace": "System.Data.SQLite",
      /*是否启用事务*/
      "IsTransaction": true,
      /*运行超时*/
      "CommandTimeOut": 0,
      /*是否缓存*/
      "IsCache": false,
      /*缓存时长 单位是秒*/
      "CacheTimeOut": 600,
      /*缓存类型 0是内存 1是磁盘*/
      "CacheType": "0",
      /*缓存目录*/
      "CachePath": "Cache",
      /*数据库连接字符串*/
      "ConnectionString": "Data Source={RootPath}App_Data\\ZW_CMS_Data_SQL.db3;Version=3;Cache Size=5000;Pooling=False;Max Pool Size=100;"
    }
  ]
}