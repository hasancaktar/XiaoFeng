{
  /*是否开启上传*/
  "IsOpen": true,
  /*是否检测文件木马*/
  "IsCheckTrojan": true,
  /*木马特征*/
  "TrojanFeature": "request[\\[\\(\\s]|eval|alert|document\\.|response.|system|excute|redirect|delete |create |exec |select |update |\\.asp|\\.php|\\.aspx|\\.ashx|\\.jsp|\\.asmx|\\.js| table | file|for\\(|foreach|drop |alter |dbo\\.|sys\\.|<script|\\.getfolder|\\.createfolder|\\.deletefolder|\\.createdirectory|\\.deletedirectory|\\.saveas|wscript\\.shell|script\\.encode|server\\.|\\.createobject|execute|activexobject|language=|echo|<\\?php",
  /*最大允许上传大小*/
  "MaxLength": 10485760,
  /*生成文件名格式*/
  "FileNameFormat": "ZW_{yyyy}{MM}{dd}{HH}{mm}{ss}{fff}_{rnd4}.{ext}",
  /*上传目录*/
  "UploadPath": "/UploadFiles",
  /*默认允许上传类型*/
  "Other": {
    "Path": "Other",
    "Ext": "html|htm|txt|xml|jpg|jpeg|gif|bmp|png|rar|zip|7z|doc|xls|ppt|pdf|wmv|wma|flv|mp3|mp4|wpeg|swf"
  },
  /*各种类型配置*/
  "Data": {
    /*数据库*/
    "Back": {
      "Path": "BakPath",
      "Ext": ""
    },
    /*图片*/
    "Image": {
      "Path": "Image",
      "Ext": "jpg|jpeg|gif|bmp|png"
    },
    /*视频*/
    "Video": {
      "Path": "Video",
      "Ext": "wmv|flv|mp3|mp4|wpeg|swf"
    },
    /*资源*/
    "Down": {
      "Path": "Down",
      "Ext": "rar|zip|doc|xls|ppt|pdf|apk|ipa"
    },
    /*广告*/
    "AD": {
      "Path": "AD",
      "Ext": "jpg|jpeg|bmp|png|gif|swf"
    },
    /*模板*/
    "Template": {
      "Path": "Template",
      "Ext": "html|htm|txt"
    }
  },
  "ImageCompressEnable": true, /*是否启用图片压缩*/
  "ImageCompressLength": 1000, /*图片压缩最长边限制*/
  "ImageCompressQuality": 90, /*图片压缩质量*/
  "IsDomain" /*上传路径是否带域名*/: true,
  "Domain" /*域名*/: "http://image.fayelf.com"
}